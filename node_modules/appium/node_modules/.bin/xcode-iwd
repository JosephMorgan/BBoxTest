#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/bash" ]; then
  "$basedir/bash"  "$basedir/../appium-ios-driver/bin/xcode-iwd.sh" "$@"
  ret=$?
else 
  bash  "$basedir/../appium-ios-driver/bin/xcode-iwd.sh" "$@"
  ret=$?
fi
exit $ret
